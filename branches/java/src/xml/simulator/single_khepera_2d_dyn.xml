<!DOCTYPE SimCom SYSTEM "../../dtd/SimCom.dtd">
<!--
  $Author: zahedi $
-->


<SimCom>
  <InputParameter>
    <Parameter Name="MotorLeft"/>
    <Parameter Name="MotorRight"/>
  </InputParameter>
  <OutputParameter>
    <!-- obstacles from -1 to 1, -1 = far away, or not seen -->
    <Parameter Name="Distance0"/>
    <Parameter Name="Distance1"/>
    <Parameter Name="Distance2"/>
    <Parameter Name="Distance3"/>
    <Parameter Name="Distance4"/>
    <Parameter Name="Distance5"/>
    <Parameter Name="Distance6"/>
    <Parameter Name="Distance7"/>

    <!-- light intensity from -1 to 1, -1 = dark, 1 = bright -->
    <Parameter Name="Light0"/>
    <Parameter Name="Light1"/>
    <Parameter Name="Light2"/>
    <Parameter Name="Light3"/>
    <Parameter Name="Light4"/>
    <Parameter Name="Light5"/>
    <Parameter Name="Light6"/>
    <Parameter Name="Light7"/>
  </OutputParameter>
  <CommunicationPort>
    10000
  </CommunicationPort>
</SimCom>
